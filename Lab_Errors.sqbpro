CREATE VIEW vw_OpenBugs AS
SELECT 
    b.BugID,
    b.Title,
    b.Status,
    b.AssignedTo,
    b.CreatedAt,
    e.ErrorDescription,
    e.Severity
FROM BugReport b
JOIN ErrorLog e ON b.ErrorID = e.ErrorID
WHERE b.Status != 'Resolved';</sql><current_tab id="0"/></tab_sql></sqlb_project>
