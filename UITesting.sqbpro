SELECT 'Users with empty login' as Test, COUNT(*) as ProblemCount 
FROM Users WHERE Login IS NULL OR trim(Login) = ''
UNION ALL
SELECT 'Transactions with negative amount' as Test, COUNT(*) as ProblemCount 
FROM Transactions WHERE Amount &lt; 0
UNION ALL
SELECT 'Students without grades' as Test, COUNT(*) as ProblemCount 
FROM Students s LEFT JOIN Grades g ON s.ID = g.StudentID WHERE g.ID IS NULL;</sql><current_tab id="0"/></tab_sql></sqlb_project>
